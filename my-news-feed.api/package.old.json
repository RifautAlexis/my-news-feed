{
  "name": "my-news-feed.api",
  "version": "0.0.0",
  "license": "MIT",
  "description": "For fun and learn on my free time",
  "author": "Alexis Rifaut",
  "repository": "git@github.com:RifautAlexis/my-news-feed.git",
  "scripts": {
    "dev": "ts-node-dev app.ts",
    "dev:db": "docker compose -f docker-compose.yml up -d",
    "prettier": "prettier --write '**/*.{js,ts}'",
    "test": "jest --config .jest.config.js --no-cache --detectOpenHandles",
    "prod:build": "node ./build.js",
    "prod:start": "prisma generate && prisma migrate deploy && export NODE_ENV=production; node ./dist/index.js"
  },
  "private": true,
  "engines": {
    "node": "16.15.0"
  },
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.1",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-multer": "^1.0.2",
    "koa-router": "^10.1.1",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.6.9",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@types/koa": "^2.13.4",
    "@types/koa-bodyparser": "^4.3.7",
    "@types/koa-router": "^7.4.4",
    "@types/node": "^17.0.34",
    "esbuild": "^0.14.39",
    "esbuild-node-externals": "^1.4.1",
    "jest": "^28.1.0",
    "prisma": "^3.14.0",
    "ts-jest": "^28.0.2",
    "typescript": "^4.6.4",
    "ts-node-dev": "^1.1.8"
  }
}
